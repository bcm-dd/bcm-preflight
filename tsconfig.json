{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@forms/*": [
        "./src/forms/*"
      ],
      "@providers": [
        "./src/providers"
      ],
      "@providers/*": [
        "./src/providers/*"
      ],
      "@utilities/*": [
        "./src/utilities/*"
      ],
      "@form": [
        "./src/form"
      ],
      "@form/*": [
        "./src/form/*"
      ],
      "@fields/*": [
        "./src/fields/*"
      ],
      "@helpers": [
        "./src/helpers"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@state": [
        "./src/state"
      ],
      "@state/*": [
        "./src/state/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@types": [
        "./src/payload-types.ts"
      ],
      "@graphql": [
        "./src/graphql"
      ],
      "@graphql/*": [
        "./src/graphql/*"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
